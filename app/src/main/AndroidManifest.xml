<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.smart.androidutils">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.sharepreference.SPActivity">
        </activity>
        <activity android:name=".activity.spider.SpiderActivity">
        </activity>
        <activity android:name=".activity.classactivity.ClassActivity">
        </activity>
        <activity android:name=".activity.reflect.ReflectActivity">
        </activity>
        <activity android:name=".BaseActivity">
        </activity>
        <activity android:name=".activity.device.DeviceActivity">
        </activity>
        <activity android:name=".activity.ad.NoAdActivity">
        </activity>
    </application>

</manifest>
